<h2><strong>Usuarios</strong></h2>
<hr>
<div class="container">

    {{#each allUsers}}
    <div class="row">
        <ul>
            <li>
                <p>{{this.username}}</p>
                <div class="row">
                    <form action="/eliminar?user_id={{_id}}" method="POST">
                        <button type="submit" class="btn btn-dark btn-sm">Borrar</button>
                    </form>
                </div>
            </li>
        </ul>
    </div>
    {{/each}}

</div>